{
  "name": "create-fay-app",
  "version": "1.0.0",
  "keywords": [
    "react",
    "next",
    "fay"
  ],
  "description": "Create Fay apps with no build configuration.",
  "main": "index.js",
  "author": "love-fay",
  "bin": {
    "create-fay-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "prepublish": "npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/love-fay/create-fay-app.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/love-fay/create-fay-app/issues"
  },
  "homepage": "https://github.com/love-fay/create-fay-app#readme",
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/cross-spawn": "6.0.2",
    "@types/node": "^12.6.8",
    "@types/prompts": "2.0.1",
    "@types/rimraf": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/validate-npm-package-name": "3.0.0",
    "@zeit/ncc": "^0.20.4",
    "async-retry": "1.3.1",
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "got": "10.7.0",
    "make-dir": "3.0.0",
    "prompts": "2.1.0",
    "rimraf": "3.0.0",
    "tar": "4.4.18",
    "typescript": "3.8.3",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0"
  }
}
